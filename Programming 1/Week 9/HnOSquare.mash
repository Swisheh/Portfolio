** Description: A program that prints #'s and O's in a square
*/


import console;


print("Enter a number: ");
int number = readInt();
for (int loop2 = 0; loop2 < number; loop2 = loop2+1){
	println("");
	for (int loop = 0; loop < number; loop = loop+1 ){
		if ((loop+loop2) % 2 == 0){
			print("X");
		} else {
			print("O");
		}
	}
}
println("");